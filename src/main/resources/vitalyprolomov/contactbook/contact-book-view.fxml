<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vitalyprolomov.contactbook.ContactBookController">

    <children>
        <TableView fx:id="contactsTableView" prefHeight="418.0" prefWidth="298.0">
            <columns>
                <TableColumn fx:id="nameColumn" prefWidth="145.5999755859375" text="Name" />
                <TableColumn fx:id="surnameColumn" prefWidth="151.99997558593748" text="Surname" />
            </columns>
        </TableView>
        <VBox layoutX="336.0" layoutY="38.0" prefHeight="323.0" prefWidth="108.0" spacing="30.0">
            <children>
                <Label fx:id="nameLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="20.0" prefWidth="60.0" text="Name">
                    <VBox.margin>
                        <Insets top="10.0" />
                    </VBox.margin>
                </Label>
                <Label fx:id="surnameLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="20.0" prefWidth="60.0" text="Surname">
                    <VBox.margin>
                        <Insets top="10.0" />
                    </VBox.margin>
                </Label>
                <Label fx:id="telephoneLabel" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0" prefHeight="20.0" prefWidth="60.0" text="Telephone">
                    <VBox.margin>
                        <Insets top="10.0" />
                    </VBox.margin>
                </Label>
                <Label fx:id="cityLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="20.0" prefWidth="60.0" text="City">
                    <VBox.margin>
                        <Insets top="10.0" />
                    </VBox.margin>
                </Label>
                <Label fx:id="postalCodeLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="20.0" prefWidth="80.0" text="Postal Code">
                    <VBox.margin>
                        <Insets left="-10.0" right="10.0" top="10.0" />
                    </VBox.margin>
                </Label>
                <Label fx:id="birthdayLabel" alignment="CENTER" contentDisplay="CENTER" minHeight="20.0" minWidth="60.0" text="Birthday">
                    <VBox.margin>
                        <Insets top="10.0" />
                    </VBox.margin>
                </Label>
            </children>
            <padding>
                <Insets left="20.0" />
            </padding>
        </VBox>
        <VBox layoutX="444.0" layoutY="38.0" prefHeight="323.0" prefWidth="108.0" spacing="20.0">
            <children>
                <Label fx:id="nameTextLabel" alignment="CENTER" contentDisplay="CENTER" minHeight="30.0" prefHeight="40.0" prefWidth="80.0" text="-">
                    <VBox.margin>
                        <Insets top="5.0" />
                    </VBox.margin>
                </Label>
                <Label fx:id="surnameTextLabel" alignment="CENTER" contentDisplay="CENTER" minHeight="30.0" prefHeight="40.0" prefWidth="80.0" text="-">
                    <VBox.margin>
                        <Insets />
                    </VBox.margin>
                </Label>
                <Label fx:id="telepphoneTextLabel" alignment="CENTER" contentDisplay="CENTER" minHeight="30.0" prefHeight="40.0" prefWidth="80.0" text="-">
                    <VBox.margin>
                        <Insets />
                    </VBox.margin>
                </Label>
                <Label fx:id="cityTextLabel" alignment="CENTER" contentDisplay="CENTER" minHeight="30.0" prefHeight="40.0" prefWidth="80.0" text="-">
                    <VBox.margin>
                        <Insets />
                    </VBox.margin>
                </Label>
                <Label fx:id="postalCodeTextLabel" alignment="CENTER" contentDisplay="CENTER" minHeight="30.0" prefHeight="40.0" prefWidth="80.0" text="-">
                    <VBox.margin>
                        <Insets top="5.0" />
                    </VBox.margin>
                </Label>
                <Label fx:id="birthdayTextLabel" alignment="CENTER" minHeight="30.0" minWidth="60.0" prefHeight="40.0" prefWidth="80.0" text="-">
                    <VBox.margin>
                        <Insets top="5.0" />
                    </VBox.margin>
                </Label>
            </children>
            <padding>
                <Insets left="20.0" />
            </padding>
        </VBox>
        <Label fx:id="personDetailsLabel" layoutX="406.0" layoutY="14.0" text="Person Details" />
      <Button fx:id="editContactButton" layoutX="318.0" layoutY="378.0" mnemonicParsing="false" text="edit" />
      <Button fx:id="addContactButton" layoutX="419.0" layoutY="378.0" mnemonicParsing="false" text="add new" />
      <Button fx:id="exitButton" layoutX="534.0" layoutY="378.0" mnemonicParsing="false" text="exit" />
    </children>

</AnchorPane>
